---
{
	"permalink": "/docs/db.json",
	"eleventyImport": {
		"collections": ["docs"]
	}
}
---

{% capture json %}
[
	{% for docs_page in collections.docs %}
		{
			"title": "{{ docs_page.data.title | replace: '"', '\\"' }}",
			"terms": "{{ docs_page.data.terms }}",
			"url": "{{ docs_page.data.page.url }}"
		}{% unless forloop.last %},{% endunless %}
	{% endfor %}
]
{% endcapture %}

{{ json | jsonparse | json }}
